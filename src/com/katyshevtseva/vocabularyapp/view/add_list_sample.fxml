<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?scenebuilder-stylesheet /css/style?>

<?import java.net.URL?>
<AnchorPane xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml" fx:controller="com.katyshevtseva.vocabularyapp.controller.AddListController">

    <stylesheets>
        <URL value="@/css/style.css"/>
    </stylesheets>

    <HBox AnchorPane.leftAnchor="10" AnchorPane.rightAnchor="10" AnchorPane.topAnchor="10">
        <Label text="Enter list name: "/>
        <TextField fx:id="nameField" onAction="#ok2" HBox.hgrow="ALWAYS"/>
    </HBox>


    <Button text="OK" onMouseClicked="#ok1" AnchorPane.rightAnchor="10" AnchorPane.bottomAnchor="10"/>
</AnchorPane>
